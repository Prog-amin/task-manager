{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://REPLACE_WITH_RAILWAY_DOMAIN/api/:path*"
    },
    {
      "source": "/socket.io/:path*",
      "destination": "https://REPLACE_WITH_RAILWAY_DOMAIN/socket.io/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
